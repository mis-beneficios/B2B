{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "anhskohbo/no-captcha": "^3.0",
        "aws/aws-sdk-php": "^3.277",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "barryvdh/laravel-snappy": "^1.0",
        "fideloper/proxy": "^4.4",
        "firebase/php-jwt": "^5.0",
        "fruitcake/laravel-cors": "^2.0",
        "graham-campbell/flysystem": "^7.1",
        "graham-campbell/markdown": "^13.1",
        "guzzlehttp/guzzle": "^7.0",
        "intervention/image": "^2.7",
        "jenssegers/date": "^4.0",
        "jubaer/zoom-laravel": "^1.3",
        "laravel/framework": "^8.0",
        "laravel/legacy-factories": "^1.3",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-cached-adapter": "~1.0",
        "league/flysystem-sftp": "~1.0",
        "league/mime-type-detection": "^1.14",
        "livewire/livewire": "^2.12",
        "maatwebsite/excel": "^3.1",
        "macsidigital/laravel-api-client": "^3.0",
        "macsidigital/laravel-zoom": "^5.0",
        "mailersend/laravel-driver": "^1.0",
        "mailgun/mailgun-php": "^3.5",
        "mediconesystems/livewire-datatables": "^0.9.4",
        "nyholm/psr7": "^1.5",
        "openpay/sdk": "dev-master",
        "php-http/guzzle7-adapter": "^0.1",
        "php-http/message-factory": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^2.4",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-backup": "^6.16",
        "stripe/stripe-php": "^10.17",
        "symfony/http-client": "^5.4",
        "webpatser/laravel-uuid": "^4.0",
        "yajra/laravel-datatables-oracle": "^9.18"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": ["database/seeds", "database/factories"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": ["Illuminate\\Foundation\\ComposerScripts::postAutoloadDump", "@php artisan package:discover --ansi", "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump", "@php artisan package:discover --ansi", "@php artisan vendor:publish --force --tag=livewire:assets --ansi"],
        "post-root-package-install": ["@php -r \"file_exists('.env') || copy('.env.example', '.env');\""],
        "post-create-project-cmd": ["@php artisan key:generate --ansi"]
    }
}
